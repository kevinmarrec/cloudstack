{
  "name": "@kevinmarrec/cloudstack-vue-i18n",
  "type": "module",
  "version": "1.0.0-beta.8",
  "description": "Opinionated Internationalization (i18n) Vue plugin.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/cloudstack/tree/main/packages/vue-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/cloudstack.git",
    "directory": "packages/vue-i18n"
  },
  "keywords": [
    "cloudstack",
    "i18n",
    "internationalization",
    "vue",
    "vue-i18n"
  ],
  "exports": {
