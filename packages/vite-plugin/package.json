{
  "name": "@kevinmarrec/cloudstack-vite-plugin",
  "type": "module",
  "version": "1.0.0-rc.8",
  "description": "Opinionated Vite plugin.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/cloudstack/tree/main/packages/vite-plugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/cloudstack.git",
    "directory": "packages/vite-plugin"
  },
  "keywords": [
    "cloudstack",
    "vite",
    "vite-plugin"
