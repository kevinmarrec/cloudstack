{
  "name": "@kevinmarrec/cloudstack-vue",
  "type": "module",
  "version": "1.0.0-rc.8",
  "description": "Opinionated Collection of Vue Components and Composition Utilities.",
  "author": "Kevin Marrec <kevin@marrec.io>",
  "license": "MIT",
  "homepage": "https://github.com/kevinmarrec/cloudstack/tree/main/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinmarrec/cloudstack.git",
    "directory": "packages/vue"
  },
  "keywords": [
    "cloudstack",
    "composables",
    "composition",
    "utils",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./head": {
      "types": "./dist/head.d.ts",
      "import": "./dist/head.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./pwa": {
      "types": "./dist/pwa.d.ts",
      "import": "./dist/pwa.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean --dts --format esm src/index.ts src/components.ts src/head.ts src/i18n.ts src/pwa.ts"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "dependencies": {
    "@kevinmarrec/cloudstack-vue-i18n": "workspace:*",
    "@kevinmarrec/cloudstack-vue-pwa": "workspace:*",
    "@unhead/vue": "^2.0.2",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0"
  }
}
